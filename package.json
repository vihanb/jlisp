{
  "name": "jlisp",
  "version": "0.0.1",
  "description": "Lisp dialect",
  "main": "bin/jlisp",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tuxcrafting/jlisp.git"
  },
  "bin": {
    "jlisp": "lib/cli/jlisp.js"
  },
  "scripts": {
    "lint": "eslint src",
    "build": "babel src -d lib -s",
