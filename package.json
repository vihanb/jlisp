  "license": "GPL-3.0",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "eslint": "^3.9.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-standard": "^2.0.1",
    "jsdoc": "^3.4.2",
    "marked": "^0.3.6"
  },
  "dependencies": {
    "commander": "^2.9.0",
    "prompt-sync": "^4.1.4",
    "prompt-sync-history": "^1.0.1"
  }
}
